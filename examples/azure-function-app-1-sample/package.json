{
  "name": "azure-function-app-1-sample",
  "type": "module",
  "main": "dist/main.js",
  "private": true,
  "scripts": {
    "ts": "ts-node --files -r dotenv/config",
    "build": "npx tsc -p tsconfig.build.json",
    "start:func": "npx func start -p 8080",
    "start:azurite": "azurite -l ./.azurite",
    "start:tsc-watcher": "yarn build -w",
    "prestart": "yarn build",
    "start": "TZ=UTC concurrently -r -p none --kill-others 'yarn run start:azurite -s' 'yarn start:tsc-watcher' 'yarn run start:func'"
  },
  "dependencies": {
    "@azure/functions": "^4.0.1",
    "@bluish/azure": "workspace:^",
    "@bluish/core": "workspace:^",
    "@bluish/cron": "workspace:^",
    "@bluish/http": "workspace:^",
    "@bluish/multipart": "workspace:^",
    "@bluish/typeorm": "workspace:^",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commander": "^11.0.0",
    "http-errors": "^2.0.0",
    "omac": "0.0.0-alpha.2",
    "sql.js": "^1.8.0",
    "typeorm": "latest"
  },
  "packageManager": "yarn@3.6.3",
  "devDependencies": {
    "@types/http-errors": "^2",
    "@types/sql.js": "^1",
    "azure-functions-core-tools": "^4.0.5413",
    "azurite": "^3.26.0",
    "concurrently": "^8.2.1",
    "ts-node": "^10.9.1"
  },
  "version": "0.0.0-a033a75"
}
